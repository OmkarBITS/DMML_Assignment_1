{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DMML-A1-ADF"
		},
		"DMML_A1_DBX_LS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'DMML_A1_DBX_LS'"
		},
		"DMML_A1_ADLS_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DMML_A1_ADLS_LS'"
		},
		"DMML_A1_DBX_LS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0301-165526-tsnb6fvu"
		},
		"DMML_A1_ADLS_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dmmla1adls.dfs.core.windows.net/"
		},
		"DMML_A1_AML_LS_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "9860037c-e3ac-41f5-a8e8-5d282d9693b2"
		},
		"DMML_A1_AML_LS_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "DMML_Assignment_1"
		},
		"DMML_A1_AML_LS_properties_typeProperties_mlWorkspaceName": {
			"type": "string",
			"defaultValue": "DMML-A1-AML"
		},
		"DMML_A1_GitHub_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DMML-A1-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DMML_A1_DBX_LS_accessToken": {
						"value": "[parameters('DMML_A1_DBX_LS_accessToken')]"
					},
					"DMML_A1_ADLS_LS_accountKey": {
						"value": "[parameters('DMML_A1_ADLS_LS_accountKey')]"
					},
					"DMML_A1_DBX_LS_properties_typeProperties_existingClusterId": {
						"value": "[parameters('DMML_A1_DBX_LS_properties_typeProperties_existingClusterId')]"
					},
					"DMML_A1_ADLS_LS_properties_typeProperties_url": {
						"value": "[parameters('DMML_A1_ADLS_LS_properties_typeProperties_url')]"
					},
					"DMML_A1_AML_LS_properties_typeProperties_subscriptionId": {
						"value": "[parameters('DMML_A1_AML_LS_properties_typeProperties_subscriptionId')]"
					},
					"DMML_A1_AML_LS_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('DMML_A1_AML_LS_properties_typeProperties_resourceGroupName')]"
					},
					"DMML_A1_AML_LS_properties_typeProperties_mlWorkspaceName": {
						"value": "[parameters('DMML_A1_AML_LS_properties_typeProperties_mlWorkspaceName')]"
					},
					"DMML_A1_GitHub_LS_properties_typeProperties_url": {
						"value": "[parameters('DMML_A1_GitHub_LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}